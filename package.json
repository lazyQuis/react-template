{
  "name": "react-template",
  "version": "1.0.0",
  "description": "React Template",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/totofish/react-template.git"
  },
  "scripts": {
    "dev": "gulp webpack-dev-server",
    "release": "gulp && webpack --release && gulp manifest && node server.js --release",
    "addlibs": "yarn add git+https://github.com/totofish/react-libs.git",
    "cp": "yarn run cp:console && yarn run cp:es5-sham && yarn run cp:es5-shim && yarn run cp:es6-promise && yarn run cp:es6-shim && yarn run cp:fetch && yarn run cp:xhr",
    "cp:console": "cp -f node_modules/console-polyfill/index.js src/assets/scripts/console-polyfill.js",
    "cp:es5-sham": "cp -f node_modules/es5-shim/es5-sham.min.js src/assets/scripts",
    "cp:es5-shim": "cp -f node_modules/es5-shim/es5-shim.min.js src/assets/scripts",
    "cp:es6-promise": "cp -f node_modules/es6-promise/dist/es6-promise.min.js src/assets/scripts",
    "cp:es6-shim": "cp -f node_modules/es6-shim/es6-shim.min.js src/assets/scripts",
    "cp:fetch": "cp -f node_modules/fetch-polyfill/fetch.js src/assets/scripts",
    "cp:xhr": "cp -f node_modules/xhr-polyfill/dist/xhr-polyfill.js src/assets/scripts"
  },
  "author": "totofish",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.3.0",
    "gulp-manifest": "^0.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-open": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.24.0",
    "image-webpack-loader": "^3.0.0",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "node-sass": "^3.10.1",
    "opener": "^1.4.2",
    "parallelshell": "^2.0.0",
    "portfinder": "^1.0.9",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "console-polyfill": "^0.2.3",
    "domready": "^1.0.8",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "fetch-polyfill": "^0.8.2",
    "jsonp": "^0.2.0",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-libs": "git+https://github.com/totofish/react-libs.git",
    "react-redux": "^4.4.5",
    "react-router": "2.8.1",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-saga": "^0.12.0",
    "xhr-polyfill": "^0.1.8"
  }
}
