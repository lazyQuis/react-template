{
  "presets": [
    ["es2015", {"modules": false}],
    "stage-0",
    "react"
  ],
  "plugins": [
    "babel-plugin-transform-decorators-legacy",
    "react-hot-loader/babel"
  ],
  "env": {
    "development": {
      "presets": []
    },
    "test": {
      "presets": ["es2015", "stage-0", "react"],
      "plugins": [
        "babel-plugin-transform-decorators-legacy",
        [ "babel-plugin-webpack-alias", { "config": "./webpack.config.mocha.js" } ]
      ]
    }
  }
}
